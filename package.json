{
	"name": "gitversionview",
	"displayName": "GitVersion View",
	"description": "Display the actual git version of the current branch.",
	"version": "0.1.9",
	"publisher": "HSSE-Development",
	"engines": {
		"vscode": "^1.107.0"
	},
	"keywords": [
		"GIT",
		"GitVersion",
		"Version",
		"Versioning",
		"Branch"
	],
	"license": "MIT",
	"homepage": "https://github.com/HSSE-Dev/GitVersionView",
	"bugs": {
		"url": "https://github.com/HSSE-Dev/GitVersionView/issues",
		"email": "hsse-development@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/HSSE-Dev/GitVersionView.git"
	},
	"categories": [
		"Other"
	],
	"icon": "images/package_icon.png",
	"activationEvents": [
		"*"
	],
	"extensionDependencies": [
		"vscode.git"
	],
	"contributes": {
		"configuration": [
			{
				"type": "object",
				"title": "GitVersion View",
				"properties": {
					"gitVersionView.versionFormat": {
						"type": "string",
						"default": "SemVer",
						"description": "Specify the version format. Example: InformationalVersion, SemVer, MajorMinorPatch..."
					},
					"gitVersionView.command": {
						"type": "string",
						"default": "gitversion",
						"description": "Specify the command of gitversion. Example: gitversion, dotnet-gitversion"
					}
				}
			}
		],
		"commands": [
			{
				"command": "extension.gitVersionView",
				"title": "Show GitVersion"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.107.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "22.x",
		"typescript-eslint": "^8.48.1",
		"eslint": "^9.39.1",
		"typescript": "^5.9.3",
		"@vscode/test-cli": "^0.0.12",
		"@vscode/test-electron": "^2.5.2"
	},
	"dependencies": {
		"@electron/rebuild": "^4.0.2"
	}
}
